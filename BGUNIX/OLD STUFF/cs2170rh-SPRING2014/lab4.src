	ORG 000	;Addison Snyder
	ENTRY	;Set entry point
	LDA A	;Load A into AC
	ADD B	;ADD B TO AC
	CMA	;COMPLEMENT AC
	INC	;INCREMENT AC
	SNA	;SKIP NEXT IF NEGATIVE
	LDA C	;LOAD C INTO AC
	BUN X	;BRANCH TO X
	LDA D	;LOAD D INTO AC
X,	AND E I	;AND E/AC INDIRECTLY
	DMP	;DUMP MEMORY
	HLT	;HALT THE PROGRAM
A,	DEC 1	;Store '1' at A
B,	DEC 2	;Store '2' at B
C,	HEX FFFF ;Store 'FFFF' at C
D,	HEX BB44 ;Store 'BB44' at D
E,	HEX 'DAT ;Complement of DAT at E
DAT,	HEX ABCF ;Store 'ABCF' AT DAT
