	ORG 100	;Addison Snyder
	ENTRY
	LDA N1	;Load operand at A
	CMA	;COMPLEMENT AC
	ADD N2	;ADDS N2 TO COMPLEMENTED N1
	SNA	;SKIP NEXT IF AC NEGATIVE
	STA A	;STORE AC IN A
	SPA	;SKIP NEXT IF AC POSITIVE
	STA B	;PUT AC INTO B
	DMP	;DUMP CONTENTS
	HLT	;END
N1,	DEC 10	;STORE 10 AT N1
N2,	DEC 5	;STORE 5 AT N2
A,	DEC 0	;STORE 0 AT A
B,	DEC 0	;STORE 0 AT B
	END
